{
    "name": "Animmendation_Dataset_Handler",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "dataset_handler",
    "workspaceFolder": "/home/animmendation/tcc_animmendation/dataset_handler",
    "postCreateCommand": "chmod +x .devcontainer/entrypoint.sh && .devcontainer/entrypoint.sh",
    "extensions": [
        "mads-hartmann.bash-ide-vscode",
        "EditorConfig.EditorConfig",
        "christian-kohler.path-intellisense",
        "redhat.vscode-yaml",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "zxh404.vscode-proto3"
    ],
    "remoteUser": "animmendation",
    "remoteEnv": {
        "PYTHONPATH": "/home/animmendation/tcc_animmendation/dataset_handler/__pypackages__/3.10/lib"
    },
    "features": {
        "ghcr.io/devcontainers/features/common-utils:1": {
            "version": "latest",
            "installZsh": false,
            "installOhMyZsh": false,
            "username": "animmendation",
            "uid": 1000
        },
        "ghcr.io/guiyomh/features/fish:0": {
            "version": "latest"
        }
    }
}
